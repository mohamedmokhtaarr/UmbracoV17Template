{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
        "Content": {
            "MacroErrors": "Throw"
        },
        "ModelsBuilder": {
            "ModelsMode": "SourceCodeManual",
            "AcceptUnsafeModelsDirectory": true,
            "ModelsDirectory": "../../mohammed.mokhtar/MyProject/Models/Gen",
            "ModelsNamespace": "MyProject.Model.Gen"
        },
        "Hosting": {
            "Debug": true
        }
    }
  }
}
